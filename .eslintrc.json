{
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "func-names": "off",
        "no-console": "off",
        "prefer-destructuring": "off",
        "camelcase": ["error", { "allow": ["*_token"] }]
    }
}
